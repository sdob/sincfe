{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  ecmaFeatures: {
    jsx: true
  },
  "globals": {
    "window": true,
    "document": true
  },
  "plugins": [
  ],
  "extends": "eslint-config-airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "id-length": 0,
    "react/prop-types": 0,
    "no-use-before-define": ["error", { "functions": false }]
  }
}
