{
  "name": "sinc-fe",
  "version": "0.0.1",
  "description": "",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint 'app/**/*.@(js|jsx)'",
    "lint:msg": "validate-commit-msg",
    "test": "jest",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack -p --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-jest": "^18.0.0",
    "babel-runtime": "6.x",
    "findup": "^0.1.5",
    "http-status-codes": "^1.0.6",
    "jest": "^18.0.0",
    "moment": "^2.17.1",
    "react": "15.x",
    "react-cookie": "^1.0.4",
    "react-datepicker": "^0.39.0",
    "react-dom": "15.x",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-test-renderer": "^15.4.1",
    "redux": "^3.6.0",
    "redux-auth": "^0.0.5-beta5",
    "redux-form": "^6.2.1",
    "redux-thunk": "^2.1.0",
    "sematable": "^1.2.2"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "commitizen": "^2.5.0",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.23.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "2.x",
    "eslint-plugin-react": "latest",
    "node-sass": "^3.13.0",
    "open-browser-webpack-plugin": "0.0.2",
    "sass-loader": "^4.0.2",
    "style-loader": "~0.13.0",
    "validate-commit-msg": "2.x",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": ".coverage"
  },
  "keywords": [
    "es6"
  ],
  "author": "sdob",
  "license": "closed",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
